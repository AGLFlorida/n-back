{
  "name": "n-back",
  "main": "expo-router/entry",
  "version": "1.0.2",
  "build": "6",
  "scripts": {
    "check-versions": "ts-node scripts/check-versions.ts",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:submit": "eas build --platform ios --clear-build-number",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "doctor": "npx expo-doctor && npx expo install --check",
    "doctor:fix": "yes | npx expo install --check",
    "build:ios": "echo 'DONTUSE' && return && eas build --platform ios",
    "build:android": "echo 'DONTUSE' && return && eas build --platform android",
    "generate:icons": "bash scripts/generate_icons.sh -p ./nback-logo.png && echo '\u001b[1;31mMove generated icons to assets/images/\u001b[0m'",
    "generate:screenshots": "bash scripts/take_screenshots.sh",
    "build:clean": "rm -rf dist .expo ios android && npx expo export && npx expo prebuild --clean && ts-node scripts/update-versions.ts && npm run check-versions",
    "build:android:local": "bash scripts/copy-keystore.sh && ts-node scripts/inject-signing-config.ts && cd android && ./gradlew clean && NODE_ENV=production ./gradlew bundleRelease && ./gradlew signingReport && cd -"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "expo": "~52.0.38",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.5",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-localization": "~16.0.1",
    "expo-router": "~4.0.18",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.8",
    "expo-toast": "^1.0.3",
    "expo-updates": "~0.27.3",
    "expo-web-browser": "~14.0.2",
    "i18next": "^24.2.2",
    "jest-expo": "~52.0.3",
    "react": "19.0.0",
    "react-dom": "18.3.1",
    "react-i18next": "^15.4.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-logs": "^5.3.0",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  }
}
